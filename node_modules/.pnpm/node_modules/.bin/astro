#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/neeldave/Documents/GitHub Repositories/create-neel/node_modules/.pnpm/astro@5.16.15_@types+node@20.19.30_jiti@2.6.1_lightningcss@1.30.2_rollup@4.56.0_typescript@5.9.3/node_modules/astro/node_modules:/Users/neeldave/Documents/GitHub Repositories/create-neel/node_modules/.pnpm/astro@5.16.15_@types+node@20.19.30_jiti@2.6.1_lightningcss@1.30.2_rollup@4.56.0_typescript@5.9.3/node_modules:/Users/neeldave/Documents/GitHub Repositories/create-neel/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/neeldave/Documents/GitHub Repositories/create-neel/node_modules/.pnpm/astro@5.16.15_@types+node@20.19.30_jiti@2.6.1_lightningcss@1.30.2_rollup@4.56.0_typescript@5.9.3/node_modules/astro/node_modules:/Users/neeldave/Documents/GitHub Repositories/create-neel/node_modules/.pnpm/astro@5.16.15_@types+node@20.19.30_jiti@2.6.1_lightningcss@1.30.2_rollup@4.56.0_typescript@5.9.3/node_modules:/Users/neeldave/Documents/GitHub Repositories/create-neel/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../astro/astro.js" "$@"
else
  exec node  "$basedir/../astro/astro.js" "$@"
fi
